0065                     writeln (#186);
0066                     if dia<=meses[mes] then
0067                         begin
0068                             writeln (#204, borde, #206, borde, #206, borde, #206, borde, #206, borde, #206, borde, #206, borde, #185)
0069                         end	
0070                     else
