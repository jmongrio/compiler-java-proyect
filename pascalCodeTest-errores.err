0083             if tecla = 0 then
0084                 begin
0085                     case ord (readkey) of
0086                         72: inc (year);
0087                         80: dec (year);
0088                         77:
0089                         if mes<11 then
0090                             begin
0091                                 inc (mes);
0092                             end	
